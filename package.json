{
  "name": "bn-mallorca",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "generate:openapi": "openapi-typescript open-api.v1.json --output ./src/generated/radio.openapi.ts",
    "prepare": "husky install",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check --log-level warn .",
    "format:fix": "prettier --write --log-level warn ."
  },
  "devDependencies": {
    "@tsconfig/node18": "18.2.1",
    "@tsconfig/strictest": "2.0.1",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.4",
    "@types/jest-when": "3.5.2",
    "@types/lambda-log": "3.0.1",
    "@types/node": "20.5.6",
    "aws-cdk": "2.93.0",
    "eslint": "8.48.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-when": "3.6.0",
    "jest-mock-extended": "3.0.5",
    "lint-staged": "14.0.1",
    "openapi-typescript": "6.5.3",
    "prettier": "3.0.2",
    "ts-jest": "29.1.1",
    "ts-node": "https://github.com/TypeStrong/ts-node",
    "typescript": "5.2.2"
  },
  "overrides": {
    "tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.409.0",
    "@aws-sdk/client-secrets-manager": "3.409.0",
    "@aws-sdk/client-lambda": "3.409.0",
    "@aws-sdk/client-s3": "3.409.0",
    "@aws-sdk/s3-request-presigner": "3.409.0",
    "@aws-sdk/client-sns": "3.409.0",
    "@aws-sdk/lib-dynamodb": "3.409.0",
    "@spotify/web-api-ts-sdk": "1.1.1",
    "aws-cdk-lib": "2.95.1",
    "constructs": "10.2.70",
    "lambda-log": "3.1.0",
    "axios": "1.5.0",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,yml,yaml,json}": [
      "npx prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix"
    ]
  }
}
