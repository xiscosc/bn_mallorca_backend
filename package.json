{
  "name": "bn-mallorca",
  "version": "0.1.0",
  "bin": {
    "cdk": "bin/cdk.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "generate:openapi": "openapi-typescript open-api.v1.json --output ./src/generated/radio.openapi.ts",
    "prepare": "husky install",
    "lint:check": "eslint .",
    "lint:fix": "eslint --fix .",
    "format:check": "prettier --check --log-level warn .",
    "format:fix": "prettier --write --log-level warn ."
  },
  "devDependencies": {
    "@swc/core": "1.7.42",
    "@swc/helpers": "0.5.13",
    "@tsconfig/node18": "18.2.4",
    "@tsconfig/strictest": "2.0.5",
    "@types/aws-lambda": "8.10.119",
    "@types/jest": "29.5.4",
    "@types/jest-when": "3.5.2",
    "@types/lambda-log": "3.0.1",
    "@types/lodash": "4.17.13",
    "@types/luxon": "3.4.2",
    "@types/node": "22.8.6",
    "aws-cdk": "2.176.0",
    "esbuild": "0.19.11",
    "eslint": "8.56.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "17.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-unicorn": "48.0.1",
    "husky": "9.1.6",
    "jest": "29.7.0",
    "jest-mock-extended": "3.0.5",
    "jest-when": "3.6.0",
    "lint-staged": "15.2.10",
    "openapi-typescript": "7.4.2",
    "prettier": "3.1.1",
    "regenerator-runtime": "0.14.1",
    "ts-jest": "29.1.1",
    "tsx": "4.19.1",
    "typescript": "5.7.3"
  },
  "overrides": {
    "tsconfig-paths": "4.2.0"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.731.1",
    "@aws-sdk/client-lambda": "3.731.1",
    "@aws-sdk/client-s3": "3.731.1",
    "@aws-sdk/client-secrets-manager": "3.731.1",
    "@aws-sdk/client-sns": "3.731.1",
    "@aws-sdk/client-sqs": "3.731.1",
    "@aws-sdk/lib-dynamodb": "3.731.1",
    "@spotify/web-api-ts-sdk": "1.2.0",
    "aws-cdk-lib": "2.176.0",
    "axios": "1.7.7",
    "constructs": "10.4.2",
    "lambda-log": "3.1.0",
    "lodash": "4.17.21",
    "luxon": "3.5.0",
    "source-map-support": "0.5.21"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,yml,yaml,json}": [
      "npx prettier --write"
    ],
    "**/*.{js,jsx,ts,tsx}": [
      "npx eslint --fix"
    ]
  }
}
